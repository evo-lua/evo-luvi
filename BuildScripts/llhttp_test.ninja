# Automatically generated by evo-ninja (editing this file is pointless)
ninja_required_version = 1.11
include_dirs = -I../deps/llhttp-ffi/llhttp/include 
rule compile
  command = gcc -MMD -MT $out -MF $out.d -c $in $include_dirs -o $out
  description = Compiling $out
  depfile = $out.d
  deps = gcc
rule bcsave
  command = luajit -b $in $out
  description = Generating optimized bytecode $out
  deps = luajit
build api.c.o: compile ../deps/llhttp-ffi/llhttp/src/api.c
  includes = $include_dirs
build http.c.o: compile ../deps/llhttp-ffi/llhttp/src/http.c
  includes = $include_dirs
build llhttp.c.o: compile ../deps/llhttp-ffi/llhttp/src/llhttp.c
  includes = $include_dirs
